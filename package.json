{
  "name": "savestate.gg",
  "private": true,
  "workspaces": ["frontend", "backend"],
  "scripts": {
    "install:all": "npm install --prefer-dedupe",
    "dev": "concurrently -n web,api -c blue,magenta \"npm run dev -w frontend\" \"npm run dev -w backend\"",
    "build": "npm run build --workspaces",
    "lint": "npm run lint --workspaces",
    "dedupe": "npm dedupe"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
